<% provide(:title, 'Users') %>
<h1>Users</h1>

<%= will_paginate %>

<ul class="user-list">
  <% @users.each do |user| %>
      <li class='user-list'>
        <div class='table'>
            <div class='name'>      <%= user.name %> </div>
            <div class='email'>     <%= user.email %> </div>
            <div class='view'>      <%= link_to image_tag("view.svg" , title: "View User" , size: 32 ), user_path(user) %> </div>
        </div>
      </li>
  <% end %>
</ul>

<%= will_paginate %>

